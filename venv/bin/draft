#!/home/jbsouth/Documents/draft-app/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'draft','console_scripts','draft'
__requires__ = 'draft'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('draft', 'console_scripts', 'draft')()
    )
